{
  "name": "media-vitals-hcp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl --ssl-key ./ssl/localhost.key  --ssl-cert ./ssl/localhost.crt --configuration=localhost",
    "start:dev": "ng serve --ssl --ssl-key ./ssl/localhost.key  --ssl-cert ./ssl/localhost.crt --configuration=development",
    "start:stage": "ng serve --ssl --ssl-key ./ssl/localhost.key  --ssl-cert ./ssl/localhost.crt --configuration=testing",
    "start:prod": "ng serve --ssl --ssl-key ./ssl/localhost.key  --ssl-cert ./ssl/localhost.crt --configuration=production",
    "build": "ng build",
    "watch": "ng build --watch --configuration localhost",
    "test": "ng test --browsers=Chrome --code-coverage",
    "test:ci": "ng test --browsers=ChromeHeadless",
    "gen-cer": "openssl req -new -x509 -newkey rsa:2048 -sha256 -nodes -keyout ./ssl/localhost.key -days 3560 -out ./ssl/localhost.crt -config ./ssl/openssl.config.cnf",
    "add-cert-windows": "certutil.exe -addstore root ./ssl/localhost.crt",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts4": "^4.6.1",
    "@amcharts/amcharts5": "^5.10.1",
    "@angular/animations": "^15.2.9",
